				{
					"name": "Login - Success",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test('Response contains token', function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('token');",
									"    pm.expect(jsonData.token).to.be.a('string').and.not.empty;",
									"});",
									"",
									"pm.test('Response contains expiration date', function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('expirationAtUtc');",
									"});",
									"",
									"// Store the token for subsequent authenticated requests",
									"const jsonData = pm.response.json();",
									"if (jsonData.token) {",
									"    pm.collectionVariables.set('authToken', jsonData.token);",
									"    ",
									"    // Decode JWT to get user ID",
									"    const tokenParts = jsonData.token.split('.');",
									"    if (tokenParts.length === 3) {",
									"        const payload = JSON.parse(atob(tokenParts[1]));",
									"        const userId = payload['http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier'] || payload.nameid || payload.sub;",
									"        if (userId) {",
									"            pm.collectionVariables.set('userId', userId);",
									"        }",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
